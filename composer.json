{
  "name": "statikbe/craft-cookie-banner",
  "description": "Cookie banner and modal package",
  "type": "craft-plugin",
  "version": "3.3.2",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "cookies",
    "gdpr"
  ],
  "support": {
    "docs": "https://github.com/statikbe/craft-cookie-banner/blob/master/README.md",
    "issues": "https://github.com/statikbe/craft-cookie-banner/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Statik",
      "homepage": "https://www.statik.be"
    },
    {
      "name": "Jan Henckens",
      "homepage": "https://github.com/janhenckens/"
    }
  ],
  "require": {
    "craftcms/cms": "^4.0.0",
    "jaybizzle/crawler-detect": "^1.2"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "statikbe\\cookiebanner\\": "src/"
    }
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G",
    "ci": "ecs check --ansi --fix && phpstan --memory-limit=1G"
  },
  "extra": {
    "name": "Cookie Banner",
    "handle": "cookie-banner",
    "schemaVersion": "1.0.0",
    "hasSettings": false,
    "developer": "Statik",
    "class": "statikbe\\cookiebanner\\CookieBanner"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
